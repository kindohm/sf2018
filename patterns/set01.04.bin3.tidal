bps (140/120)

let 
    env1 = 3
    hpf1 = 4 
    rel1 = 8
    det1 = 9
    env2 = 64
    hpf2 = 65
    rel2 = 66
    det2 = 67
    dfb = 68

do
    let pat = (density 2 $ slowcat ["0(3,8)", "~", "0*4?", "1"])
    let prog = "[0 -2]/4"
    -- synths
    a1 $ (|*| gain 1.1) $ (|+| note prog) $ gain (pat) # note "[0,[4 7]/3,11]" # midichan 15 # s "midi"
    a2 $ (|*| gain 1.15) $ (|+| note prog) $ gain (inv <$> pat) # note "0" # midichan 12 # s "midi" # sustain 0.1
    cc $ stack [ 
            gain "1*4" # control (scale 30 30 $ slow 13.1 sine) # ctlNum env1
            , gain "1*4" # control (scale 0 0 $ slow 11.31 sine) # ctlNum hpf1
            , gain "1*4" # control (scale 0 40 $ slow 12.25 sine) # ctlNum rel1
            , gain "1*4" # control (scale 0 10 $ slow 11.1258 sine) # ctlNum det1
            , gain "1*4" # control (scale 10 10 $ slow 10.212 sine) # ctlNum env2
            , gain "1*4" # control (scale 0 0 $ slow 8.41 sine) # ctlNum hpf2
            , gain "1*4" # control (scale 0 10 $ slow 9.587 sine) # ctlNum rel2
            , gain "1*4" # control (scale 0 10 $ slow 10.518 sine) # ctlNum det2
            , gain "1*4" # control (scale 0 10 $ slow 7.918 sine) # ctlNum dfb
            ] # s "midi" # midichan 15 # midicmd "control"
    d1 $ stack [            
                gain (inv <$> pat) # s "artk" # rel 0.3,
                superimpose ((#pan 0) . (|*| speed 0.8)) $ 
                    gain (pat) # s "cp" # speed "0.8" # pan 1 # hpf 1000
        ]

        hush
